{
  "rewrites": [
    {
      "source": "/old-url",
      "destination": "/new-url"
    }
  ],
  "headers": [
    {
      "source": "/api/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=3600"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": true
}
